{
  "name": "exek", 
  "version": "1.0.0",
  "description": "Quickly Search and execute shortcuts",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "Arman Jindal & Sambhav Anand",
  "license": "ISC",
  "devDependencies": {
    "electron": "^8.5.1",
    "electron-builder": "^22.6.1"
  },
  "build": {
    "appId": "Exek",
    "mac": {
      "category": "public.app.category.productivity"
    },
    "extraResources": [
      {
        "from": "assets",
        "to": "resources",
        "filter": [
            "**/*"
        ]
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "dependencies": {
    "applescript": "^1.0.0",
    "electron-fetch": "^1.4.0",
    "menubar": "^8.0.2",
    "moment": "^2.25.3",
    "querystring": "^0.2.0"
  }
}
